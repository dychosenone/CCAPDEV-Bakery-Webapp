<%- include('./partials/head.ejs'); %>
<body>
<div class="container-js">
    <%- include('./partials/header.ejs'); %>

    <section class="hero hero-header-background is-small">
        <div class="hero-body has-text-centered">
            <p class="hero-styling">Admin Credentials</p>
        </div>
    </section>

    <div class="container" style="margin-top: 60px; margin-bottom: 40px">
        <form class="admin-edit-account">
            <div class="has-text-justified" style="margin-left: 5%; margin-top: 40px;">
                <label style="margin-left: 20%;"><b>Username</b></label>
                <input type="text" disabled class="input input-styled" style="width: 300px" value="<%=result.username%>"><br><br>

                <a href="/admin/admin-edit/<%=result._id%>" class="button submit-button" style="margin-bottom: 10px; margin-left: 41.5%;">Edit</a>

            </div>
            <input type="hidden" name="Username" id="Username" class="input input-styled" style="width: 300px" value="<%=result.username%>" >

        </form>
    </div>

    <%- include('./partials/footer.ejs'); %>

</div>
</body>
</html>